{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "ClearBaseAcls": true
  },
  "properties": {
    "firstname": {
      "type": "string"
    },
    "middlename": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "customers": {
      "type": "hasMany",
      "model": "Customer",
      "through": "CustomerUserMap"
    },
    "hubs": {
      "type": "hasMany",
      "model": "Hub",
      "through": "HubUserMap"
    },
    "dailyMktPrices": {
      "type": "hasMany",
      "model": "DailyMktPrice",
      "foreignKey": "userId"
    },
    "dailyMktPriceHistory": {
      "type": "hasMany",
      "model": "DailyMktPriceHistory",
      "foreignKey": "userId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "roleMappings": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId"
    },
    "customerMappings": {
      "type": "hasMany",
      "model": "CustomerUserMap",
      "foreignKey": "userId"
    },
    "hubMappings": {
      "type": "hasMany",
      "model": "HubUserMap",
      "foreignKey": "userId"
    },
    "purchaseOrders": {
      "type": "hasMany",
      "model": "PurchaseOrder",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
